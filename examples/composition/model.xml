<model namespace="examples.composition">
	<type name="Person">
		<component name="homeAddress" type="Address" />
    <component name="workAddress" type="Address" />
    <component name="externalAddress" type="Address" receiveInConstructor="true" />
  </type>
	<type name="Address">
		<property name="street" type="string" />
		<property name="city" type="string" />
		<property name="zipCode" type="string" />
	</type>
</model>